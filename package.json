{
  "name": "my-nuxt-practice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue,.jsx,.tsx --ignore-path .gitignore . --fix",
    "lint:style": "stylelint **/*.{vue,tsx,jsx,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/http": "^0.6.4",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/composition-api": "^0.24.4",
    "@nuxtjs/dotenv": "^1.4.1",
    "@tailwindcss/postcss7-compat": "^2.2.7",
    "@vue/runtime-core": "^3.2.4",
    "autoprefixer": "^9.8.6",
    "core-js": "^3.6.5",
    "csstype": "^3.0.8",
    "element-ui": "^2.15.6",
    "nuxt": "^2.14.6",
    "postcss": "^7.0.36",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "babel-preset-vca-jsx": "^0.3.6",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.1.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4",
    "vuex-module-decorators": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
